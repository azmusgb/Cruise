{
  "defaults": {
    "ignoreClasses": [
      "open",
      "show",
      "visible",
      "is-*",
      "fa-*",
      "env-depth-*",
      "animate-*",
      "mobile-*",
      "count-chip",
      "grid",
      "list"
    ]
  },
  "pages": {
    "decks.html": {
      "ignoreClasses": ["active"],
      "ignoreIds": ["deckPlanCanvas", "deckPlanStage"]
    },
    "contacts.html": {
      "ignoreIds": ["ocean", "sharedHeader"],
      "ignoreClasses": ["contact-context--port", "contact-section--priority", "copy-success", "port"]
    },
    "dining.html": {
      "ignoreClasses": ["deck-card--context-pick", "is-loading"]
    },
    "index.html": {
      "ignoreClasses": ["offline"]
    },
    "itinerary.html": {
      "ignoreClasses": ["is-active", "is-live", "is-today", "no-anim", "on"],
      "ignoreIds": ["sharedHeader", "today-card"]
    },
    "operations.html": {
      "ignoreClasses": ["is-complete"],
      "ignoreIds": ["ocean", "sharedHeader"]
    },
    "photos.html": {
      "ignoreIds": ["sharedHeader"],
      "ignoreClasses": ["gallery-list-view", "img-shimmer", "photos-lightbox-open"]
    },
    "plan.html": {
      "ignoreIds": ["ocean", "sharedHeader"]
    },
    "rooms.html": {
      "ignoreIds": ["sharedHeader"],
      "ignoreClasses": [
        "all",
        "compare-toggle-btn",
        "filter-btn--active",
        "room-card__status-bar",
        "room-card__status-line",
        "modal-prev-room",
        "modal-next-room",
        "modal-continue-room"
      ]
    },
    "tips.html": {
      "ignoreClasses": ["is-complete", "search-status-loading"]
    }
  }
}
